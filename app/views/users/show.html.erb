<h1>User Details</h1>	
<div class="row">
<div class="col-md-4">
	<%= image_tag "interior_view.jpg", class: 'image-car' %>
</div>
<div class="col-md-4 col-md-offset-1">
	<ul class="list-group users-show" >
		<li class="list-group-item">Fist name : <%= @user.first_name %></li>
		<li class="list-group-item">Last name : <%= @user.last_name %></li>
		<li class="list-group-item">Id card : <%= @user.id_card %></li>
		<li class="list-group-item">Mobile : <%= @user.mobile %></li>
		<li class="list-group-item">Email : <%= @user.email %></li>
		<li class="list-group-item">Company : <%= @user.company %></li>
	</ul>
</div>
</div>
<%= link_to 'Back', :back, class: "btn btn-warning"%>
