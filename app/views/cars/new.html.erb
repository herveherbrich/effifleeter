<div class="row">
	<h2><span class="title-sections">&nbsp;&nbsp; New Car &nbsp;&nbsp;</span></h2>
</div>
<div class="well">
	<div class="row">
		<%= form_for @car, url: user_cars_path(@user), class: 'form-horizontal' do |f| %>
			<div class="form-group">
				<%= f.label :tag, class: "control-label col-md-3" %>
				<div class="col-md-9">
					<%= f.text_field :tag, class: "form-control" %>
				</div>
			</div>
			<br><br><br>
			<div class="form-group">
				<%= f.label :frame, class: "control-label col-md-3" %>
				<div class="col-md-9">
					<%= f.text_field :frame, class: "form-control" %>
				</div>
			</div>
			<br><br>
			<div class="form-group">
				<%= f.label :brand, class: "control-label col-md-3" %>
				<div class="col-md-9">
					<%= f.text_field :brand, class: "form-control" %>
				</div>
			</div>
			<br><br>
			<div class="form-group">
				<%= f.label :model, class: "control-label col-md-3" %>
				<div class="col-md-9">
					<%= f.text_field :model, class: "form-control" %>
				</div>
			</div>
			<br><br>
			<div class="form-group">
				<%= f.label :finishing, class: "control-label col-md-3" %>
				<div class="col-md-9">
					<%= f.text_field :finishing, class: "form-control" %>
				</div>
			</div>
			<br><br>
			<div class="form-group">
				<%= f.label :numb_places, class: "control-label col-md-3" %>
				<div class="col-md-9">
					<%= f.text_field :numb_places, class: "form-control" %>
				</div>
			</div>
			<br><br>
			<div class="form-group">
				<%= f.label :color, class: "control-label col-md-3" %>
				<div class="col-md-9">
					<%= f.text_field :color, class: "form-control" %>
				</div>
			</div>
			<br><br>
			<div class="form-group">
				<%= f.label :registration_date, class: "control-label col-md-3" %>
				<div class="col-md-9">
					<%= f.text_field :registration_date, class: "form-control" %>
				</div>
			</div>
			<br><br>
			<div class="form-group">
				<%= f.label :discharge_date, class: "control-label col-md-3" %>
				<div class="col-md-9">
					<%= f.text_field :discharge_date, class: "form-control" %>
				</div>
			</div>
			<br><br>
			<div class="form-group">
				<%= f.label :leaving_date, class: "control-label col-md-3" %>
				<div class="col-md-9">
					<%= f.text_field :leaving_date, class: "form-control" %>
				</div>
			</div>
			<br><br>
			<div class="form-group">
				<%= f.label :kms, class: "control-label col-md-3" %>
				<div class="col-md-9">
					<%= f.text_field :kms, class: "form-control" %>
				</div>
			</div>
			<br>
			<div class="btn-group pull-right">
				<br>
				<%= link_to 'Back', :back, class: "btn btn-warning" %>
				<%= f.submit class: "btn btn-default" %>
			</div>
		<% end %>
	</div>
</div>
	

