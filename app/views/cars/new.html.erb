<h1>New Car</h1>

<%= form_for @car, url: user_cars_path(@user) do |f| %>
	<p>
		<%= f.label :tag %>
		<%= f.text_field :tag %>
	</p>

	<p>
		<%= f.label :frame %>
		<%= f.text_field :frame %>
	</p>

	<p>
		<%= f.label :brand %>
		<%= f.text_field :brand %>
	</p>

	<p>
		<%= f.label :model %>
		<%= f.text_field :model %>
	</p>

	<p>
		<%= f.label :finishing %>
		<%= f.text_field :finishing %>
	</p>

	<p>
		<%= f.label :numb_places %>
		<%= f.text_field :numb_places %>
	</p>

	<p>
		<%= f.label :color %>
		<%= f.text_field :color %>
	</p>

	<p>
		<%= f.label :registration_date %>
		<%= f.text_field :registration_date %>
	</p>

	<p>
		<%= f.label :discharge_date %>
		<%= f.text_field :discharge_date %>
	</p>

	<p>
		<%= f.label :leaving_date %>
		<%= f.text_field :leaving_date %>
	</p>

	<p>
		<%= f.label :kms %>
		<%= f.text_field :kms %>
	</p>

	<p>
		<%= f.submit %>
	</p>

<% end %>