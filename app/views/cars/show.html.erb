<h1>Car Details</h1>	
<div class="row">
<div class="col-md-4">
	<%= image_tag "clio.jpg", class: 'image-car' %>
</div>
<div class="col-md-4 col-md-offset-1">
	<ul class="list-group cars-show" >
		<li class="list-group-item"><%= @car.tag %></li>
		<li class="list-group-item"><%= @car.frame %></li>
		<li class="list-group-item"><%= @car.brand %></li>
		<li class="list-group-item"><%= @car.model %></li>
		<li class="list-group-item"><%= @car.finishing %></li>
		<li class="list-group-item"><%= @car.numb_places %></li>
		<li class="list-group-item"><%= @car.color %></li>
		<li class="list-group-item"><%= @car.registration_date %></li>
		<li class="list-group-item"><%= @car.discharge_date %></li>
		<li class="list-group-item"><%= @car.leaving_date %></li>
		<li class="list-group-item"><%= @car.kms %></li>
	</ul>
</div>
</div>